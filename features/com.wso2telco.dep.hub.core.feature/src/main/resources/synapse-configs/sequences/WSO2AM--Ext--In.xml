<?xml version="1.0" encoding="UTF-8"?>
<sequence name="WSO2AM--Ext--In" trace="disable" xmlns="http://ws.apache.org/ns/synapse">
    <property expression="get-property('SYSTEM_TIME')" name="REQUEST_TIMESTAMP" scope="default" type="STRING"/>
    <property expression="get-property('MessageID')" name="MESSAGE_ID" scope="default" type="STRING"/>
    <sequence key="httpHeaderSeq"/>
    <!-- Values log level = full; enable payload loggin. log level=custom; disable payload loggin for the request -->
    <log level="custom">
        <property name="API Request URL" expression="get-property('To')"/>
        <property name="Request ID" expression="get-property('MESSAGE_ID')"/>
        <property name="API" expression="$ctx:API_NAME"/>
        <property name="Version" expression="get-property('SYNAPSE_REST_API_VERSION')"/>
        <property name="Application Name" expression="$ctx:api.ut.application.name"/>i
        <property name="Application ID" expression="$ctx:api.ut.application.id"/>
        <property name="User Name" expression="$ctx:api.ut.userName"/>
    </log>
</sequence>
