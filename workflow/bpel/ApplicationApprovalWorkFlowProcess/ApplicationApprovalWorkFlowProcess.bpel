			<bpel:copy>
				<bpel:from>
					<![CDATA['admin']]>
				</bpel:from>
				<bpel:to part="AppApprovalAuditRequest" variable="input_hub_app_approval_audit">
					<bpel:query queryLanguage="urn:oasis:names:tc:wsbpel:2.0:sublang:xpath1.0"><![CDATA[axau:completedByRole]]></bpel:query>
				</bpel:to>
